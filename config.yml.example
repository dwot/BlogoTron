config:
  settings:
    image-negatives: |
      watermark,border,blurry,duplicate
    sampler-name: |
      DPM++ 2M
  prompt:
    idea-prompt: |
        Come up with 10 new ideas for articles. Return the results in unnumbered Pipe-Delimitted format.  The expected format is: "Idea"|
    title-prompt: |
      Find a title for your article. Use the language of the article.
    system-prompt: |
      You are blog writer and you want to write a new article.
    web-prompt: |
      Write an {{.Length}} word blog article about {{.Prompt}}.
    img-prompt: |
      {{.ImagePrompt}}